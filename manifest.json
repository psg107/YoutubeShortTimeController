{
    "name": "YoutubeShortTimeController",
    "description": "You can control the current time of YouTube Shorts with a mouse click.",
    "version": "1.0.0.2",
    "manifest_version": 3,
    "icons": {
        "128": "icon.png"
    },
    "author": "SGPark",
    "background": {
        "service_worker": "background.js"
    },
    "host_permissions": [
        "*://youtube.com/*",
        "*://www.youtube.com/*"
    ],
    "permissions": [
        "activeTab",
        "tabs",
        "scripting"
    ]
}